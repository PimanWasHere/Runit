<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:screenOrientation="portrait"
    tools:context=".Activityupdateprofile">
    <!--  style="@style/Widget.AppCompat.EditText"-->

    <EditText
        android:id="@+id/editTextnicknameupdate"
        android:layout_width="140dp"
        android:layout_height="44dp"
        android:ems="10"
        android:fontFamily="sans-serif-smallcaps"
        android:hint="Nickname ?.."
        android:inputType="text"
        android:textAlignment="center"
        android:textColor="#0040E0"
        android:textColorHint="#0040E0"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.046" />

    <Button
        android:id="@+id/updatedataprefbutt"
        android:layout_width="168dp"
        android:layout_height="48dp"
        android:background="@drawable/myrndredkbutton"
        android:fontFamily="sans-serif-smallcaps"
        android:text="DATA PREFERENCES"
        android:textAlignment="center"

        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        android:textStyle="bold"
        android:typeface="monospace"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.782" />

    <ProgressBar
        android:id="@+id/progressBarupdate"
        style="?android:attr/progressBarStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.819" />

    <EditText
        android:id="@+id/editTextlnameedit"
        android:layout_width="183dp"
        android:layout_height="39dp"
        android:ems="10"
        android:fontFamily="sans-serif-smallcaps"
        android:hint="Last Name ?.."
        android:inputType="text"
        android:textAlignment="center"
        android:textColor="#0040E0"
        android:textColorHint="#0040E0"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.929"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.128" />

    <EditText
        android:id="@+id/editTextfnameedit"
        android:layout_width="169dp"
        android:layout_height="39dp"
        android:ems="10"
        android:fontFamily="sans-serif-smallcaps"
        android:hint="First Name ?.."
        android:inputType="text"
        android:textAlignment="center"
        android:textColor="#0040E0"
        android:textColorHint="#0040E0"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.119"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.128" />

    <Switch
        android:id="@+id/switch10edit"
        android:layout_width="121dp"
        android:layout_height="38dp"
        android:scaleX="1.5"
        android:scaleY="1.5"
        android:text="Participant"
        android:textColor="#0040E0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.256" />

    <Switch
        android:id="@+id/switch16edit"
        android:layout_width="121dp"
        android:layout_height="38dp"
        android:scaleX="1.5"
        android:scaleY="1.5"
        android:text="Developer"
        android:textColor="#0040E0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.58" />

    <Switch
        android:id="@+id/switch15edit"
        android:layout_width="121dp"
        android:layout_height="38dp"
        android:scaleX="1.5"
        android:scaleY="1.5"
        android:text="Sponsor"
        android:textColor="#0040E0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.525" />

    <Switch
        android:id="@+id/switch14edit"
        android:layout_width="121dp"
        android:layout_height="38dp"
        android:scaleX="1.5"
        android:scaleY="1.5"
        android:text="Brand"
        android:textColor="#0040E0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.471" />

    <Switch
        android:id="@+id/switch13edit"
        android:layout_width="121dp"
        android:layout_height="31dp"
        android:scaleX="1.5"
        android:scaleY="1.5"
        android:text="Club"
        android:textColor="#0040E0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.418" />

    <Switch
        android:id="@+id/switch12edit"
        android:layout_width="121dp"
        android:layout_height="31dp"
        android:scaleX="1.5"
        android:scaleY="1.5"
        android:text="Spectator"
        android:textColor="#0040E0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.362" />

    <Switch
        android:id="@+id/switch11edit"
        android:layout_width="121dp"
        android:layout_height="31dp"
        android:scaleX="1.5"
        android:scaleY="1.5"
        android:text="Fan"
        android:textColor="#0040E0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.31" />

    <TextView
        android:id="@+id/textView3edit"
        android:layout_width="107dp"
        android:layout_height="32dp"
        android:background="#FFFFFF"
        android:fontFamily="sans-serif-condensed"
        android:text="I'm a .."
        android:textAlignment="center"
        android:textColor="@color/colorAccent"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.095"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.208" />

    <Button
        android:id="@+id/updateaccountbutt"
        android:layout_width="168dp"
        android:layout_height="48dp"
        android:background="@drawable/myrndredkbutton"
        android:fontFamily="sans-serif-smallcaps"
        android:text="UPDATE PROFILE"
        android:textAlignment="center"

        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        android:textStyle="bold"
        android:typeface="monospace"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.917" />


</androidx.constraintlayout.widget.ConstraintLayout>